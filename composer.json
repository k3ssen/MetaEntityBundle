{
    "name": "k3ssen/entity-generator",
    "description": "Helps creating entity-metadata for your symfony4 project",
    "keywords": ["wame", "entity", "entities", "meta", "metadata", "symfony", "symfony4"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Driessen",
            "email": "k3ssen@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "symfony/yaml": "~4.0",
        "symfony/config": "~4.0",
        "symfony/dependency-injection": "~4.0",
        "symfony/process": "~4.0",
        "symfony/validator": "~4.0",
        "symfony/orm-pack": "~1.0"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": { "K3ssen\\MetaEntityBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
